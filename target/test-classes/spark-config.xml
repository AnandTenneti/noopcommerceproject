<?xml version="1.0" encoding="UTF-8" ?>
<extentreports>
    <configuration>
        <!--report theme-->
        <!--STANDARD, DARK-->
        <theme>STANDARD</theme>
        <!--document editing-->
        <!--defaults to UTF-8-->
        <encoding>UTF-8</encoding>
        <!--protocol for script and stylesheets-->
        <!--defaults to https-->
        <!--HTTP, HTTPS-->
        <protocol>HTTPS</protocol>
        <!--offline report-->
        <timelineEnabled>true</timelineEnabled>
        <enableOfflineMode>true</enableOfflineMode>
        <thumbnailForBase64>true</thumbnailForBase64>
        <documentTitle>nopcommerce Admin Tests</documentTitle>
        <reportName>BDD Tests</reportName>
        <timestampFormat>MMM dd, yyyy HH:mm:ss</timestampFormat>
        <!-- custom javascript -->
        <scripts>
            <![CDATA[
                $(document).ready(function() {

                });
            ]]>
        </scripts>

        <!-- custom styles -->
        <styles>
            <![CDATA[

            ]]>
        </styles>
    </configuration>
</extentreports>